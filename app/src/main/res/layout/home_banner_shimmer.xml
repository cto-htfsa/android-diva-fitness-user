        android:background="@color/gray1"
        android:scaleType="fitXY"
        android:layout_margin="@dimen/dimen_10"
        app:siRadius="@dimen/dimen_10"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>